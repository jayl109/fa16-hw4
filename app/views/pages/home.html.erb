<table>
  <tr>
    <td>username</td>
    <td>age</td>
    <td>email</td>
    <td>created_at</td>
    <td>updated_at</td>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username%></td>
      <td><%= user.age%></td>
      <td><%= user.email%></td>
      <td><%= user.created_at%></td>
      <td><%= user.updated_at%></td>
    </tr>
  <% end %>
</table>
<table>
  <tr>
    <td>Cat name</td>
    
  </tr>
  <% @cats.each do |cat| %>
    <tr>
    
      <td><%= cat.name%></td>
    </tr>
  <% end %>
</table>